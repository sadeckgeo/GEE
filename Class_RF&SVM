/***** End of imports. If edited, may not auto-convert in the playground. *****/

// Classification Period
var date1 = '2017-07-01'
var date2 = '2017-07-31'

// Area of Interest (AOI)
var AOI = ee.FeatureCollection('ft:1tdSwUL7MVpOauSgRzqVTOwdfy17KDbw-1d9omPw') 
                .filterMetadata('Country', 'equals', 'Greece');

Map.centerObject(AOI, 6);
var AOI_nopaint = ee.Image().paint(AOI, 0, 2);
Map.addLayer(AOI_nopaint,  {color: '000000'}, "Europe");
/******************************************************************************************************/
/*** Sentinel 2 classification ***/
/******************************************************************************************************/  
// Function to add cloud index band   
var cloudindex = function(image) {
  var clouds = image.expression('1/INDEX', {'INDEX':image.metadata('CLOUD_COVERAGE_ASSESSMENT')});
  return image.addBands(clouds);
};

// Load RAW Imagery for cloud cover assessment
var imageCollectionS2 = ee.ImageCollection('COPERNICUS/S2') 
  .filterBounds(AOI)
  .filterDate(date1, date2)
  .map(cloudindex);
print (imageCollectionS2, "Collection")

var imageS2 = imageCollectionS2.qualityMosaic('constant') 
      .select(['B1', 'B2', 'B3', 'B4', 'B5', 'B6', 'B7', 'B8', 'B8A', 'B9', 'B10', 'B11', 'B12', 'QA10', 'QA20', 'QA60'])
      .clip(AOI);
      
Map.addLayer(imageS2, {bands: ['B11', 'B8', 'B4'], min: 1000, max:4535}, 'Sentinel2'); //['B5', 'B6', 'B4'] 0.3

//var newfc = gtps;// 
var classes = water.merge(thickveg).merge(thinveg).merge(clouds).merge(shadows).merge(baresoil).merge(rice).merge(cotton).merge(corn).merge(sunflower).merge(marsh).merge(grasses);//.merge(riparian)
var bands = ['B2',  'B3', 'B4', 'B5', 'B6', 'B7', 'B8', 'B8A', 'B11', 'B12'];

/**************************************************************************************************/
/*** Randon forest classification ***/
/**************************************************************************************************/
var RFtraining = imageS2.select(bands).sampleRegions({
  collection: classes, 
  properties: ['value'], 
  scale: 10
});
  
var RFclassifier = ee.Classifier.randomForest(24).train({
  features: RFtraining, 
  classProperty: 'value',
  inputProperties: bands
});
print("classifier", RFclassifier);

// Classify the input imagery.
var RFclassified = imageS2.select(bands).classify(RFclassifier);
print("RFclassified", RFclassified);
Map.addLayer(RFclassified, {min: 1, max: 12, palette: ['red', '9408ff', 'efe80f', 'ff86b8', 'd67938','blue', 'gray', 'white', 'green','84c233', '001994','66b7d6']}, 'RF classification');   //, '6ad6a9'

/**************************************************************************************************/
/*** SVM classification ***/
/**************************************************************************************************/
var SVMtraining = imageS2.select(bands).divide(1000).sampleRegions({
  collection: classes, 
  properties: ['value'], 
  scale: 10
});
print(SVMtraining.first())
// Train the classifier.
var SVMclassifier = ee.Classifier.svm({
  kernelType: 'RBF',
  gamma: 0.2,
  cost: 1
});

// Train the classifier.
var SVMtrained = SVMclassifier.train({
  features: SVMtraining, 
  classProperty:'value', 
  inputProperties: bands
});

// Classify the image.
var SVMclassified = imageS2.select(bands).divide(1000).classify(SVMtrained);
Map.addLayer(SVMclassified, {min: 1, max: 12, palette: ['red', '9408ff', 'efe80f', 'ff86b8', 'd67938','blue', 'gray', 'white', 'green','84c233', '001994','66b7d6']}, 'SVM classification', false); //, '6ad6a9' 

//Artigo?