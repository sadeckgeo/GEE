//Contagem de pontos no pol√≠gono
var nPoints = fc.map(function(poly){
  var pointsPoly = points.filterBounds(poly.geometry());
  var n = pointsPoly.size();
  return poly.set({'nPoints': n});
});
print(nPoints);

Map.centerObject(fc)