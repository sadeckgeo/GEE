//Vetor
var A = [1,3,5,7,12]
var B = [5,8,7,3,2]
var C = []
for (var i=0; i<A.length; i++) {
  C.push (A[i]+B[i])
  
} 
print (C)

//Matriz
var D = [[1,3,2], [3,9,4]]
var E = [[5,3,1], [6,7,8]]
var F = [[0,0,0],[0,0,0]]

for (var i=0; i<2; i++) {
   for (var j=0; j<3; j++) {
     F[i][j] = D[i][j]+E[i][j]
   }
}  
  
print (F)
