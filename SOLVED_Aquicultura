//Initial Parameters

var solvedLib = require('users/luizcf14/SolvedLibs:SolvedLib')
//Lib Check
solvedLib.startMessage() 

var year = 2017;

var studyArea = geometry2.intersection(estados.filterMetadata('sigla','equals','PI'),4)//municipios.filterMetadata('NM_MUNICIP','equals','ACARA');
var landsatCollection = ee.ImageCollection('LANDSAT/LC08/C01/T1')
    .filterDate('2017-01-01', '2017-12-31')
Map.addLayer(municipios,{},'Municipios',false)
//Functions
var getNotWaterClusterID = function(clusterizedImage){
  var ID = clusterizedImage.reduceRegion({
    reducer:ee.Reducer.mean(),
    geometry:NotWaterPoint,
    scale:30
  });
  ID = ID.get('cluster');
  return ee.Number.parse(ID);
}
var getWaterClusterID = function(clusterizedImage){ 
  var ID = clusterizedImage.reduceRegion({
    reducer:ee.Reducer.mean(),
    geometry:WaterPoint,
    scale:30
  });
  ID = ID.get('cluster');
  return ee.Number.parse(ID);
}
var imageClassification = null;
var vectorsMAPCollection = null;
// This demonstration uses hand-located points to train a classifier.
// Each training point has a field called 'landcover' containing
// class labels at that location. The following block contains
// construction code for the points.  Hover on the 'urban' variable
// and click, 'Convert' in the dialog.
var filterVectors = function(feature){
  feature = feature.set({'area':ee.Number(feature.area(1))});
  feature = feature.set({'perimeter':ee.Number(feature.perimeter(1))});
  feature = feature.set({'area/perimeter':ee.Number(feature.area(1)).divide(ee.Number(feature.perimeter(1)))});
  feature = feature.set({'poslby-popper':((ee.Number(4).multiply(ee.Number(3.141518))).multiply(ee.Number(feature.area(1)))).divide(ee.Number(feature.perimeter(1)).pow(ee.Number(2)))});
  return feature;
 }
 
var proximityFitler = function(feature){
  var bounds = feature.buffer(500,1);
  var vectorsInDistance = vectorsMAPCollection.filterBounds(bounds.geometry());
  feature = feature.set({'near':ee.Number(vectorsInDistance.size()).subtract(ee.Number(1))});
  return feature;
}

var buffed = function(feature){
  feature = feature.buffer(30,1);
  feature = feature.set({'areaBuf':ee.Number(feature.area(1))});
  feature = feature.set({'perimeterBuf':ee.Number(feature.perimeter(1))});
  feature = feature.set({'area/perimeterBuf':ee.Number(feature.area(1)).divide(ee.Number(feature.perimeter(1)))});
  return feature
}

var removeBuffer = function(feature){
  feature = feature.buffer(-10,1);
  return feature
}

var waterPixelsCount = function(feature){
  imageClassification = imageClassification.mask(imageClassification.eq(1));
  var count = imageClassification.reduceRegion({
    reducer: ee.Reducer.count(),
    geometry: feature.geometry(),
    crs:imageClassification.projection() ,
    scale: 30
  });
  feature = feature.set({'water':ee.Number(count.get('classification'))});
  return feature
}
var NotwaterPixelsCount = function(feature){
  imageClassification = imageClassification.mask(imageClassification.neq(1));
  var count = imageClassification.reduceRegion({
    reducer: ee.Reducer.count(),
    geometry: feature.geometry(),
    crs:imageClassification.projection() ,
    scale: 30
  });
  feature = feature.set({'notWater':ee.Number(count.get('classification'))});
  return feature
}

var waterPixelsCountBuffered = function(feature){
  imageClassification = imageClassification.mask(imageClassification.eq(1));
  var countagem = imageClassification.reduceRegion({
    reducer: ee.Reducer.count(),
    geometry: feature.geometry(),
    crs:imageClassification.projection() ,
    scale: 30
  });
  feature = feature.set({'waterBuf':ee.Number(countagem.get('classification'))});
  return feature
}

var firstFilter = function(feature){
    feature = feature.set({'PassFirst':ee.Number(feature.get('water')).subtract(ee.Number(feature.get('waterBuf')))});
    return feature;
}

var notwaterPixelsCountBuffered = function(feature){
  imageClassification = imageClassification.mask(imageClassification.neq(1));
  var count = imageClassification.reduceRegion({
    reducer: ee.Reducer.count(),
    geometry: feature.geometry(),
     crs:imageClassification.projection() ,
    scale: 30
  });
  feature = feature.set({'notWaterBuf':ee.Number(count.get('classification'))});
  return feature
}

//Main Code
Map.addLayer(studyArea,{},'Area Estudo')
var numCluster = 8;
var imageRaster = mapbiomas3.filter(ee.Filter.eq('year',year)).filter(ee.Filter.eq('mosaic',1)).filter(ee.Filter.eq('version',2)).filter(ee.Filter.eq('collection',3)).aside(print).median();

var training = imageRaster.select(['MNDWI','swir2']).sample({
    region: geometry,
    scale: 30,
    numPixels: 20000
});
 
print('Training',training.size())
var clusterer = ee.Clusterer.wekaKMeans(numCluster).train(training)
var clusterized = imageRaster.cluster(clusterer)
imageRaster = imageRaster.clip(studyArea)
Map.addLayer(imageRaster,{bands:['MNDWI'],min:50,max:171},'MNDWI')
Map.addLayer(clusterized.randomVisualizer(),{},'Kmeans',false)


var listOfBands = ee.List(['MNDWI','swir2']); 
var clusterStatistical  = solvedLib.collectStatisticsClusters(clusterized,imageRaster,listOfBands,numCluster);
clusterStatistical = ee.FeatureCollection(clusterStatistical);
print('Clusters',clusterStatistical);

//Water Cluster Choice
clusterStatistical = ee.FeatureCollection(ee.List(clusterStatistical));

//Water Points
var listOfBandsRef = ['MNDWI-MEAN','swir2-MEAN'];

var statisticalRealOfRef = {'MNDWI-MEAN':255,'swir2-MEAN':0};
print('Water')
var waterClusterSize = 4
var water_Points = solvedLib.samplingMethodgetID(listOfBands,listOfBandsRef,statisticalRealOfRef,imageRaster,clusterized,clusterStatistical,waterClusterSize,10000,90,numCluster,1)

print('Not Water')
var statisticalRealOfRef = {'MNDWI-MEAN':0,'swir2-MEAN':255};
var notWater_Points = solvedLib.samplingMethodgetID(listOfBands,listOfBandsRef,statisticalRealOfRef,imageRaster,clusterized,clusterStatistical,(numCluster-waterClusterSize),10000,90,numCluster,2)


//water_Points = ee.FeatureCollection(water_Points)
print('Water Sample Size',water_Points)

//notWater_Points = ee.FeatureCollection(notWater_Points)
print('Not Water Sample Size',notWater_Points)


//Create Mask
var WaterMask = ee.Image(0).byte()
for(var i = 0; i < waterClusterSize; i = i+1){
  var mascara = ee.Image(1).updateMask(clusterized.select('cluster').eq(water_Points.get(i).getInfo()));
  WaterMask = WaterMask.add(mascara.unmask(0));
}
Map.addLayer(WaterMask,{},'Water Mask')
//var NotWaterMask = ee.Image(0).updateMask(clusterized.neq(getWaterClusterID(clusterized)));
//var WaterMask = ee.Image(1).updateMask(clusterized.eq(getWaterClusterID(clusterized)));



/*
var pointsNotWater = imageRaster.updateMask(NotWaterMask).addBands(ee.Image(0).rename('class')).addBands(ee.Image.pixelLonLat())
.sample({region:geometry,scale:30, numPixels:5000}).map(function(f) { 
        return ee.Feature(
          ee.Geometry.Point([f.get('longitude'), f.get('latitude')]), f.toDictionary());
      }).set({'class':'0'});
var pointsWater = imageRaster.updateMask(WaterMask).addBands(ee.Image(1).rename('class')).addBands(ee.Image.pixelLonLat())
.sample({region:geometry,scale:30, numPixels:500}).map(function(f) { 
        return ee.Feature(
          ee.Geometry.Point([f.get('longitude'), f.get('latitude')]), f.toDictionary());
      });


var newFeatureCollection = pointsWater.merge(pointsNotWater);

// Training
var training = imageRaster.select(['MNDWI','blue']).sampleRegions({
  collection: newFeatureCollection,
  properties: ['class'],
  scale: 30
});


// Train a CART classifier.
var classifier = ee.Classifier.continuousNaiveBayes().train({
  features: training,
  classProperty: 'class',
});

var classified = imageRaster.classify(classifier).clip(studyArea);

Map.addLayer(classified, {min: 1, max: 6, palette: ['blue', 'red', 'green']});
*/

var classified = (WaterMask.unmask(0)).rename('classification');
Map.addLayer(classified, {},'Classified');
// Vector Conversion
var canny = ee.Algorithms.CannyEdgeDetector({
  image: classified, threshold: 2, sigma: 0.5
});
Map.addLayer(canny,{},'Canny',false);
var sampleImage = ee.Image(landsatCollection.filterBounds(geometry).first());
var projection = sampleImage.select(0).projection();

 var vectors = (classified.eq(1)).addBands(imageRaster.select('MNDWI')).reduceToVectors({
  geometry: studyArea,
  crs: projection,
  geometryType: 'polygon',
  eightConnected: false,
  labelProperty: 'classification',
  reducer: ee.Reducer.mode(),
  bestEffort:false,
  maxPixels: 1e13,
  geometryInNativeProjection:false
  
});
vectors = vectors.map(filterVectors,true);
vectors = vectors.filter(ee.Filter.gte('area',2000)).filter(ee.Filter.lte('area',800000));
//vectors = vectors.filter(ee.Filter.gte('poslby-popper',0.20))

imageClassification = classified;

vectors = vectors.map(waterPixelsCount);
imageClassification = classified;
vectors = vectors.map(NotwaterPixelsCount);
var vectorBuffed = vectors.map(buffed);
imageClassification = classified;
vectorBuffed = vectorBuffed.map(waterPixelsCountBuffered);
imageClassification = classified;
vectorBuffed = vectorBuffed.map(notwaterPixelsCountBuffered);
//Zero Filter, remove every shape who don't contains any pixel of water
vectorBuffed = vectorBuffed.filter(ee.Filter.neq('water',0));
//First Filter, the buffered shape dont have water pixel count difference from the original shape
vectorBuffed = vectorBuffed.map(firstFilter);
vectorBuffed = vectorBuffed.filter(ee.Filter.gte('PassFirst',-5)).filter(ee.Filter.lte('PassFirst',5));
//Second Filter, remove every shape who the principal body have much more length than width, using poslby-popper > 0.2

//vectorBuffed = vectorBuffed.filter(ee.Filter.gte('poslby-popper',0.158))
vectorBuffed = vectorBuffed.filter(ee.Filter.gte('area/perimeter',17.5))
//Third Filter, proximidade
vectorsMAPCollection = vectorBuffed;
vectorBuffed = vectorBuffed.map(proximityFitler);
vectorBuffed = vectorBuffed.filter(ee.Filter.gte('near',2));

vectorBuffed = vectorBuffed.map(removeBuffer);
//vectorBuffed =vectors.filterBounds(vectorBuffed);
var aquicultura = ee.Image(6).updateMask(classified.eq(1));
aquicultura = aquicultura.updateMask(vectorBuffed.reduceToImage(['classification'],ee.Reducer.mode()));
aquicultura = aquicultura.reproject(projection,null,30);
//aquicultura = vectors.filterBounds(aquicultura);
//Map.addLayer(vectors,{color:'blue'},'Vectors',false);

var finalResult = classified.add(aquicultura.unmask(0));
//finalResult = finalResult.resample('bilinear');

//finalResult = finalResult.resample('bicubic');
//finalResult = finalResult.reduce(ee.Reducer.mode());
Map.addLayer(vectorBuffed,{styleProperty: 'classification'},'Vectors Buffered',false);
Map.addLayer(finalResult,{min:1,max:7},'Aquicultura');


 Export.image.toAsset({
    image:finalResult.byte().set({'aquiculture':1,'year':year}),
    description:'Aquiculture_'+year+'_MABA',
    assetId: 'users/luizcf14/Aquicultura/Aquiculture_'+year+'_MABA',
    scale: 30,
    maxPixels:10000000000000,
    region: studyArea.bounds(1)
  });  
